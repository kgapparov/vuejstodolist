;AAAA,UACR,KADQ,GACY,OADZ,CACR,KADQ;AAAA,UACD,SADC,GACY,OADZ,CACD,SADC;AAEf,MAAA,KAAK,CAAC,IAAN,CAAW,4CAAX,EAAyD;AACvD,QAAA,KAAK,EAAL,KADuD;AAEvD,QAAA,SAAQ,EAAR;AAFuD,OAAzD,EAIC,IAJD,CAIM,UAAA,GAAE;AAAA,eAAK,KAAI,CAAC,KAAL,CAAW,IAAX,CAAgB,GAAG,CAAC,IAApB,CAAL;AAAA,OAJR,EAKC,KALD,CAKO,UAAA,GAAE;AAAA,eAAK,OAAO,CAAC,GAAR,CAAY,GAAZ,CAAL;AAAA,OALT;AAMF;AAtBO,GAZI;AAoCb,EAAA,OApCa,qBAoCJ;AAAA;;AACP,IAAA,KAAK,CAAC,GAAN,CAAU,qDAAV,EACC,IADD,CACM,UAAA,GAAE;AAAA,aAAK,MAAI,CAAC,KAAL,GAAa,GAAG,CAAC,IAAtB;AAAA,KADR,EAEC,KAFD,CAEO,UAAA,GAAG;AAAA,aAAE,OAAO,CAAC,GAAR,CAAY,GAAZ,CAAF;AAAA,KAFV;AAGF;AAxCa,CAAf","sourcesContent":["<template>\n  <div id=\"app\">\n    <Header />\n    <AddTodo v-on:add-todo=\"addTodo\"/>\n    <Todos v-bind:todos=\"todos\"\n    v-on:del-todo=\"deleteItem\"\n    v-on:todo-change=\"changeItem\"\n    />\n  </div>\n</template>\n\n\n<script>\nimport Todos from '../components/Todos'\nimport Header from '../components/layout/Header'\nimport AddTodo from '../components/AddTodo'\nimport axios from 'axios'\nexport default {\n  name: 'Home',\n  components: {\n    Header,\n    Todos,\n    AddTodo\n  },\n  data() {\n    return {\n      todos : []\n    }\n  },\n  methods: {\n    deleteItem(id) {\n      axios.delete(`https://jsonplaceholder.typicode.com/todos/${id}`)\n      .then( this.todos = this.todos.filter(e=> e.id != id))\n      .catch(err => console.log(err));\n    },\n    changeItem(id) {\n      for (let i=0; i<this.todos.length; i++) {\n        if (this.todos[i].id == id) {\n          this.todos[i].completed = !this.todos[i].completed\n        }\n      }\n\n    },\n    addTodo(newTodo) {\n      const {title, completed} = newTodo;\n      axios.post(\"https://jsonplaceholder.typicode.com/todos\", {\n        title, \n        completed\n      })\n      .then(res => this.todos.push(res.data))\n      .catch(err => console.log(err));\n    }\n  },\n  created(){\n    axios.get('https://jsonplaceholder.typicode.com/todos?_limit=5')\n    .then(res => this.todos = res.data)\n    .catch(err=>console.log(err));\n  }\n}\n</script>\n\n<style>\n* {\n  margin: 0;\n  padding: 0;\n  box-sizing: border-box;\n}\nbody {\n  font-family: Arial, Helvetica, sans-serif;\n  line-height: 1.4;\n}\n.btn {\n  display: inline-block;\n  border: none;\n  background-color: #555;\n  color: #fff;\n  padding: 7px 20px;\n  cursor: pointer;\n}\n.btn:hover {\n  background-color: #666;\n}\n</style>\n"],"sourceRoot":""}]}